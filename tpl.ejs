<navbar class="wi-navbar">
    <ul>
    <% for(var key in itens) {%>
        <li class="wi-navbar-item">
            <span class="wi-navbar-label"><%= itens[key].display %></span>
            
            <div class="wi-navbar-sub1">
                <% for(var keySub1 in itens[key].itens) {%>
                <div class="wi-navbar-sub1-item <% if(itens[key].itens[keySub1].class){%><%= itens[key].itens[keySub1].class %><%}%>" <% if(itens[key].itens[keySub1].onclick){%> onclick="<%= itens[key].itens[keySub1].onclick %>"<%}%>>
                    <%- itens[key].itens[keySub1].display %>
                    <% if(itens[key].itens[keySub1].shortcut){ %><span class="wi-navbar-shortcut"><%- itens[key].itens[keySub1].shortcut %></span><% } %>
                    <% if(itens[key].itens[keySub1].submenu){ %>
                        <div class="wi-navbar-submenu">
                            <i class="fa fa-caret-right" aria-hidden="true"></i>
                            <div class="wi-navbar-submenu-menu"></div>
                        </div>
                    <% } %>
                </div>
                <% if(itens[key].itens[keySub1].divide){ %><div class="wi-navbar-divide"></div><% } %>
                <% } %>
            </div>
        </li>
    <% } %>
    </ul>
</navbar>